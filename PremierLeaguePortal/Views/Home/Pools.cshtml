@model List<PremierLeaguePortal.Areas.Administration.Models.PoolViewModel>

@{ 
    ViewBag.Title = "Анкети";
}

<div class="container">
    <h1 class="text-center">Анкети</h1>
    <br/>
    <div class="row">
        <div style="clear: both"></div>
        <div class="row plp-homeCategory">
            @for (int i = 0; i < Model.Count(); i++)
            {
                @plp_Helpers.PoolPreview(Model[i], Url)
            }
        </div>
        <div style="clear: both"></div>
    </div>
</div>
